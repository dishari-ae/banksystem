{
  "name": "banksystem",
  "version": "0.0.0",
  "engines": {
    "node": "14.16.0",
    "npm": "7.12.0"
  },
  "description": "Bank system built with the MERN stack",
  "main": "app.js",
  "private": true,
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "app": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run app\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Dishari Das",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "concurrently": "^6.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "gh-pages": "^3.1.0",
    "http-errors": "~1.6.3",
    "mongoose": "^5.12.7",
    "morgan": "~1.9.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
